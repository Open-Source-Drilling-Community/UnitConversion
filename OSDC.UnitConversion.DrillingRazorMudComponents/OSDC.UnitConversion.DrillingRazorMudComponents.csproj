<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Version>1.0.0</Version>
    <Authors>Gilles Pelfrene</Authors>
    <Company>NORCE</Company>
    <Description>A package with MudBlazor components to manage unit and reference conversions.</Description>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>


  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="6.0.10" />
    <PackageReference Include="MudBlazor" Version="6.0.17" />
    <PackageReference Include="OSDC.DotnetLibraries.General.DataManagement" Version="1.0.2" />
    <PackageReference Include="OSDC.UnitConversion.Conversion" Version="1.0.3" />
    <PackageReference Include="OSDC.UnitConversion.DrillingRazorComponents" Version="1.0.14" />
  </ItemGroup>

</Project>
